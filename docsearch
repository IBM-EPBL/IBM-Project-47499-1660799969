    <script type="text/javascript">
      /**
	      * Fix for anchor links
	      * - Fix for anchor links in navs -> all DOM needs to be loaded (window.onload)
	      * - Fix for anchors on Safari mobile ->  needs scrollIntoView to the hash
        * - Fix for navigation browser buttons -> scrollIntoView to the hash and title
	    **/
      window.onload = function(){
        window.onhashchange = (event) => {
          if (window.location.hash){
            document.querySelector(window.location.hash).scrollIntoView(true);
          }
          else{
            const pageTitle = document.querySelector('h1');
            if(pageTitle){
              pageTitle.scrollIntoView(true);
            }
          }
        }
      }
    </script>
    
    
  
