    <span class="prism-token prism-keyword prism-keyword-print">print</span><span class="prism-token prism-punctuation">(</span>response<span class="prism-token prism-punctuation">.</span>body<span class="prism-token prism-punctuation">)</span>
    <span class="prism-token prism-keyword prism-keyword-print">print</span><span class="prism-token prism-punctuation">(</span>response<span class="prism-token prism-punctuation">.</span>headers<span class="prism-token prism-punctuation">)</span>
