    sg <span class="prism-token prism-operator">=</span> <span class="prism-token prism-function">SendGridAPIClient</span><span class="prism-token prism-punctuation">(</span>os<span class="prism-token prism-punctuation">.</span>environ<span class="prism-token prism-punctuation">.</span><span class="prism-token prism-function">get</span><span class="prism-token prism-punctuation">(</span><span class="prism-token prism-string">'SENDGRID_API_KEY'</span><span class="prism-token prism-punctuation">)</span><span class="prism-token prism-punctuation">)</span>
    response <span class="prism-token prism-operator">=</span> sg<span class="prism-token prism-punctuation">.</span><span class="prism-token prism-function">send</span><span class="prism-token prism-punctuation">(</span>message<span class="prism-token prism-punctuation">)</span>
